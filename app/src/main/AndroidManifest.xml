<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shindejayesharun.jetpack_compose">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Jetpack_compose">
        <activity
            android:name=".examples.TextViewSampleActivity"
            android:exported="true"
            android:label="@string/title_activity_text_view_sample"
            android:theme="@style/Theme.Jetpack_compose.NoActionBar" />
        <activity
            android:name=".examples.SimpleTextActivity"
            android:exported="true"
            android:label="@string/title_activity_simple_text"
            android:theme="@style/Theme.Jetpack_compose.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>